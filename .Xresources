! base16 ocean.dark
*.foreground:   #c0c5ce
*.background:   #2b303b
*.cursorColor:  #c0c5ce
! black
*.color0:       #2b303b
*.color8:       #65737e
! red
*.color1:       #bf616a
*.color9:       #bf616a
! green
*.color2:       #a3be8c
*.color10:      #a3be8c
! yellow
*.color3:       #ebcb8b
*.color11:      #ebcb8b
! blue
*.color4:       #8fa1b3
*.color12:      #8fa1b3
! magenta
*.color5:       #b48ead
*.color13:      #b48ead
! cyan
*.color6:       #96b5b4
*.color14:      #96b5b4
! white
*.color7:       #c0c5ce
*.color15:      #eff1f5

! font 
!URxvt.font:			xft:RobotoMono Nerd Font:size=12
URxvt.font:             	xft:UbuntuMono Nerd Font:pixelsize=19
!URxvt.font:                     xft:Source Code Pro:pixelsize=17
URxvt.letterSpace:		-1
URxvt.boldFont:			xft:UbuntuMono Nerd Font:bold:pixelsize=19
URxvt.italicfont:		xft:UbuntuMono Nerd Font:italic:pixelsize=19
URxvt.bolditalicFont:		xft:UbuntuMono Nerd Font:bold:italic:pixelsize=19
URxvt*allow_bold: 		true

! font setting
Xft.autohint: true 
Xft.antialias: true 
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

! misc
URxvt*iconFile: 		/usr/share/icons/Papirus-Light/32x32/apps/terminal.svg
URxvt.scrollBar:		false
URxvt.depth:			32
!URxvt.background:		[70]#2d2d2d
URxvt*cursorBlink: 		true
URxvt.url-select.launcher:	Firefox,chromium
URxvt.url-select-underline:	true
URxvt.imLocale:			en_US.utf8

! State:           'bg',     'fg',     'bgalt',  'hlbg',   'hlfg'
!rofi.color-normal: #6388ad,  #c7ccd1,  #6388ad,  #6388ad,  #00bcd4
!rofi.color-urgent: #6388ad,  #c7ccd1,  #6388ad,  #6388ad,  #00bcd4
!rofi.color-active: #6388ad,  #c7ccd1,  #6388ad,  #6388ad,  #00bcd4
